{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/channels/([^/]+)/videos",
      "dest": "/api/channels/[channelId]/videos.js?channelId=$1"
    },
    {
      "src": "/api/channels/([^/]+)/info",
      "dest": "/api/channels/[channelId]/info.js?channelId=$1"
    },
    {
      "src": "/api/search/channels",
      "dest": "/api/search/channels.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
